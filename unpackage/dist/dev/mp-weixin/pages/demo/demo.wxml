<view class="page"><view class="box" data-index="0" data-event-opts="{{[['tap',[['click',['$event']]]]]}}" bindtap="__e">按钮</view><view data-event-opts="{{[['tap',[['box1event',['$event']]]]]}}" class="box1" bindtap="__e">外面<view data-event-opts="{{[['tap',[['box2event',['$event']]]]]}}" class="box2" catchtap="__e">里面</view></view><view class="font">{{name}}</view><view class="font">{{''+num+''}}</view><view class="font">{{''+(num>10?"优秀":"差")+''}}</view><view class="font">{{''+zweihight+''}}</view><view data-event-opts="{{[['tap',[['change',['$event']]]]]}}" class="box" bindtap="__e">修改</view><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="box" bindtap="__e">增加</view></view>