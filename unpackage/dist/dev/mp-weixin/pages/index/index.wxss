.page{
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.uni-swiper-tab{
	border-bottom: 1rpx solid #EEEEEE;
}
.swiper-tab-list{
	color: #969696;
	font-weight: bold;
}
.uni-swiper-tab .active{
	color: #343434;
}
.active .swiper-tab-line{
	border-bottom: 6rpx solid #fede33;
	width: 70rpx;
	margin: 0 auto;
	border-top:  6rpx solid #fede33;
	border-radius: 6rpx;
}
.page {
	height: 100%;
	width: 100%;
